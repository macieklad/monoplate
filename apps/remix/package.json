{
  "name": "remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "format": "prettier --write . --ignore-path ../../.gitignore",
    "lint": "eslint ./app --max-warnings 0",
    "lint:fix": "eslint ./app --max-warnings 0 --fix",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/node": "^2.11.0",
    "@remix-run/react": "^2.11.0",
    "@remix-run/serve": "^2.11.0",
    "isbot": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@acme/components": "workspace:*",
    "@acme/style-guide": "workspace:*",
    "@acme/tailwind": "workspace:*",
    "@remix-run/dev": "^2.11.0",
    "@tsconfig/remix": "^2.0.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.7",
    "typescript": "~5.5.4",
    "vite": "~5.2.13",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
