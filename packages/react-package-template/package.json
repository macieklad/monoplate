{
  "name": "@acme/react-package-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --write . --ignore-path ../../.gitignore",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@acme/style-guide": "workspace:*",
    "@acme/vite": "workspace:*",
    "@tsconfig/vite-react": "^2.0.1",
    "@types/react": "^18.3.11",
    "typescript": "~5.6.3",
    "vite": "~5.4.9",
    "vitest": "^2.1.3"
  }
}
